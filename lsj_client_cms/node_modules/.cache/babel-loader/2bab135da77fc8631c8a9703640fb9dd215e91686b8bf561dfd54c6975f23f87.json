{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  data() {\n    return {\n      categoryList: [],\n      goodsList: [],\n      pageNum: 1,\n      pageSize: 12,\n      total: 0,\n      category: '全部',\n      sort: '最新',\n      name: ''\n    };\n  },\n  mounted() {\n    this.loadCategory();\n    this.loadGoods(1);\n  },\n  methods: {\n    handleSearch() {\n      this.$router.push({\n        path: '/front/search',\n        query: {\n          name: this.name\n        }\n      });\n    },\n    loadCategory() {\n      this.$request.get('/category/selectAll').then(res => {\n        this.categoryList = res.data || [];\n      });\n    },\n    loadGoods(pageNum) {\n      // 分页查询\n      if (pageNum) this.pageNum = pageNum;\n      this.$request.get('/goods/selectFrontPage', {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize,\n          category: this.category === '全部' ? null : this.category,\n          sort: this.sort\n        }\n      }).then(res => {\n        if (res.code === '200') {\n          this.goodsList = res.data?.list;\n          this.total = res.data?.total;\n        } else {\n          this.$message.error(res.msg);\n        }\n      });\n    },\n    handleCurrentChange(pageNum) {\n      this.loadGoods(pageNum);\n    }\n  }\n};","map":{"version":3,"names":["data","categoryList","goodsList","pageNum","pageSize","total","category","sort","name","mounted","loadCategory","loadGoods","methods","handleSearch","$router","push","path","query","$request","get","then","res","params","code","list","$message","error","msg","handleCurrentChange"],"sources":["src/views/front/Home.vue"],"sourcesContent":["<template>\r\n  <div style=\"min-height: 10vh\">\r\n    <div class=\"bg\">\r\n      <div style=\"width: 750px\">\r\n        <div style=\"display: flex\">\r\n\r\n          <el-input\r\n      v-model=\"name\"\r\n      size=\"large\"\r\n      prefix-icon=\"el-icon-search\"\r\n      placeholder=\"请输入物品名称\"\r\n      @keyup.enter=\"handleSearch\"\r\n    ></el-input>\r\n\r\n    <el-button\r\n      @click=\"handleSearch\"\r\n      size=\"medium\"\r\n      type=\"primary\"\r\n      style=\"margin-left: 5px; background-color: #464646\"\r\n    >\r\n      <i class=\"el-icon-search\"></i>\r\n    </el-button>\r\n\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <div style=\"width: 70%; background-color: #fff; margin: 10px auto; padding: 20px; border-radius: 5px; position: relative\">\r\n       <!--发布物品空位-->\r\n      <div style=\"margin-bottom: 20px\">\r\n        <el-select v-model=\"category\" size=\"medium\" style=\"width: 200px\" @change=\"loadGoods(1)\">\r\n          <el-option value=\"全部\"></el-option>\r\n          <el-option v-for=\"item in categoryList\" :key=\"item.id\" :value=\"item.name\"></el-option>\r\n        </el-select>\r\n        <el-select v-model=\"sort\" size=\"medium\" style=\"width: 200px; margin-left: 10px\" @change=\"loadGoods(1)\">\r\n          <el-option value=\"最新\"></el-option>\r\n          <el-option value=\"最热\"></el-option>\r\n        </el-select>\r\n      </div>\r\n      <div>\r\n        <el-row :gutter=\"15\">\r\n          <el-col :span=\"6\" v-for=\"item in goodsList\" :key=\"item.id\">\r\n            <div style=\"margin-bottom: 20px\" class=\"goods-item\" @click=\"$router.push('/front/goodsDetail?id=' + item.id)\">\r\n              <img :src=\"item.img\" alt=\"\" style=\"width: 100%; display: block; height: 260px; margin-bottom: 10px; border-radius: 5px\">\r\n              <div style=\"font-size: 16px; height: 40px; color: #555; margin-bottom: 10px\" class=\"line2 goods-name\">{{ item.name }}</div>\r\n              <div style=\"display: flex; align-items: baseline\">\r\n                <strong style=\"color: red; font-size: 24px\">￥{{ item.price }}</strong>\r\n                <span style=\"margin-left: 20px; color: #666\">{{ item.readCount }}人浏览</span>\r\n                <span style=\"margin-left: 20px; color: #666\">{{ item.likesCount }}人点赞</span>\r\n              </div>\r\n            </div>\r\n          </el-col>\r\n        </el-row>\r\n        <div style=\"margin: 15px auto\">\r\n          <el-pagination\r\n              background\r\n              @current-change=\"handleCurrentChange\"\r\n              :current-page=\"pageNum\"\r\n              :page-sizes=\"[5, 10, 20]\"\r\n              :page-size=\"pageSize\"\r\n              layout=\"total, prev, pager, next\"\r\n              :total=\"total\">\r\n          </el-pagination>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n\r\nexport default {\r\n\r\n  data() {\r\n    return {\r\n      categoryList: [],\r\n      goodsList: [],\r\n      pageNum: 1,\r\n      pageSize: 12,\r\n      total: 0,\r\n      category: '全部',\r\n      sort: '最新',\r\n      name: ''\r\n    }\r\n  },\r\n  mounted() {\r\n    this.loadCategory()\r\n    this.loadGoods(1)\r\n  },\r\n\r\n  methods: {\r\n    handleSearch() {\r\n      this.$router.push({ path: '/front/search', query: { name: this.name } });\r\n    },\r\n    loadCategory() {\r\n      this.$request.get('/category/selectAll').then(res => {\r\n        this.categoryList = res.data || []\r\n      })\r\n    },\r\n    loadGoods(pageNum) {  // 分页查询\r\n      if (pageNum) this.pageNum = pageNum\r\n      this.$request.get('/goods/selectFrontPage', {\r\n        params: {\r\n          pageNum: this.pageNum,\r\n          pageSize: this.pageSize,\r\n          category: this.category === '全部' ? null : this.category,\r\n          sort: this.sort\r\n        }\r\n      }).then(res => {\r\n        if (res.code === '200') {\r\n          this.goodsList = res.data?.list\r\n          this.total = res.data?.total\r\n        } else {\r\n          this.$message.error(res.msg)\r\n        }\r\n      })\r\n    },\r\n    handleCurrentChange(pageNum) {\r\n      this.loadGoods(pageNum)\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.bg {\r\n  height: 250px;\r\n  background-image: url(\"@/assets/imgs/home.jpg\");\r\n  background-size: 100%;\r\n  background-position-y: center;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n}\r\n\r\n.goods-item {\r\n  cursor: pointer;\r\n}\r\n.goods-item img, .goods-name {\r\n  transition: all .2s;\r\n}\r\n.goods-item:hover img {\r\n  scale: 1.03;\r\n}\r\n.goods-item:hover .goods-name {\r\n  color: #ec3d3d !important;\r\n}\r\n</style>\r\n"],"mappings":";AAuEA;EAEAA,KAAA;IACA;MACAC,YAAA;MACAC,SAAA;MACAC,OAAA;MACAC,QAAA;MACAC,KAAA;MACAC,QAAA;MACAC,IAAA;MACAC,IAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,YAAA;IACA,KAAAC,SAAA;EACA;EAEAC,OAAA;IACAC,aAAA;MACA,KAAAC,OAAA,CAAAC,IAAA;QAAAC,IAAA;QAAAC,KAAA;UAAAT,IAAA,OAAAA;QAAA;MAAA;IACA;IACAE,aAAA;MACA,KAAAQ,QAAA,CAAAC,GAAA,wBAAAC,IAAA,CAAAC,GAAA;QACA,KAAApB,YAAA,GAAAoB,GAAA,CAAArB,IAAA;MACA;IACA;IACAW,UAAAR,OAAA;MAAA;MACA,IAAAA,OAAA,OAAAA,OAAA,GAAAA,OAAA;MACA,KAAAe,QAAA,CAAAC,GAAA;QACAG,MAAA;UACAnB,OAAA,OAAAA,OAAA;UACAC,QAAA,OAAAA,QAAA;UACAE,QAAA,OAAAA,QAAA,wBAAAA,QAAA;UACAC,IAAA,OAAAA;QACA;MACA,GAAAa,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAE,IAAA;UACA,KAAArB,SAAA,GAAAmB,GAAA,CAAArB,IAAA,EAAAwB,IAAA;UACA,KAAAnB,KAAA,GAAAgB,GAAA,CAAArB,IAAA,EAAAK,KAAA;QACA;UACA,KAAAoB,QAAA,CAAAC,KAAA,CAAAL,GAAA,CAAAM,GAAA;QACA;MACA;IACA;IACAC,oBAAAzB,OAAA;MACA,KAAAQ,SAAA,CAAAR,OAAA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}